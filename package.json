{
  "name": "gatsby-starter-hello-world-typescript",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby using TypeScript",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "gatsby": "^2.26.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^4.2.2",
    "typescript-plugin-css-modules": "^3.2.0"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.14.0",
    "prettier": "2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Igonato/gatsby-starter-hello-world-typescript"
  },
  "bugs": {
    "url": "https://github.com/Igonato/gatsby-starter-hello-world-typescript/issues"
  }
}
